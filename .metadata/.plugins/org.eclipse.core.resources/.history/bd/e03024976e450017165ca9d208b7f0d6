import org.springframework.web.servlet.ModelAndView;

@Controller
public class categoryList 
{
	@RequestMapping(value ="/CategoryList")
    public ModelAndView add() {
        
	 ModelAndView c1=new ModelAndView("CategoryList");
		return c1;
    }

}