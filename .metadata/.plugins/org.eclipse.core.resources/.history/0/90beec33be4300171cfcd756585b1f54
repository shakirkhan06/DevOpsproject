$(document).ready( function () {
	 var table = $('#categoryTable').DataTable({
			"sAjaxSource": "categorys",
			"sAjaxDataProp": "",
			"order": [[ 0, "asc" ]],
			"aoColumns": [
			    { "mData": "categoryid"},
		      { "mData": "categoryname" }
		      
							 
			]
	 });
	 
	 $(document).ready( function () {
		 var table = $('#supplierTable').DataTable({
				"sAjaxSource": "suppliers",
				"sAjaxDataProp": "",
				"order": [[ 0, "asc" ]],
				"aoColumns": [
				    { "mData": "supplierid"},
			      { "mData": "suppliername" }
			      
								 
				]
		 });
	 });